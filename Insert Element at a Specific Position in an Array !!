#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, i, value, pos;
    scanf("%d", &n);
    int arr[101]; // 1 extra space for new element

    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    scanf("%d", &value);
    scanf("%d", &pos);

    // Shift elements to right from the end
    for(i = n; i >= pos; i--)
        arr[i] = arr[i - 1];

    arr[pos - 1] = value; // Insert element (1-based index)

    // Print updated array
    for(i = 0; i <= n; i++)
        printf("%d ", arr[i]);

    return 0;
}
