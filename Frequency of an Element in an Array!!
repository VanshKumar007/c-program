#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, i, key, count = 0;
    scanf("%d", &n);
    int a[n];
    
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);
        
    scanf("%d", &key);
    
    for(i = 0; i < n; i++)
        if(a[i] == key)
            count++;
            
    printf("%d", count);
    return 0;
}
