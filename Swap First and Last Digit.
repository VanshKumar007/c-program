#include <stdio.h>

int main() {
    int n, first, last, temp, count = 0, power = 1;

    scanf("%d", &n);
    temp = n;
    last = n % 10;

    while (temp >= 10) {
        temp = temp / 10;
        count++;
        power = power * 10;
    }
    first = temp;

    n = n % power;        
    n = n / 10;          

    int swapped = last * power + n * 10 + first;

    printf("%d", swapped);
    return 0;
}
