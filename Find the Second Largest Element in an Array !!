#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, i;
    scanf("%d", &n);
    int a[n];
    
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);

    int largest = a[0];
    int second = a[0];

    // Find largest
    for(i = 1; i < n; i++) {
        if(a[i] > largest)
            largest = a[i];
    }

    // Find second largest
    second = -10001;
    for(i = 0; i < n; i++) {
        if(a[i] != largest && a[i] > second)
            second = a[i];
    }

    if(second == -10001)
        printf("No second largest element");
    else
        printf("%d", second);

    return 0;
}
