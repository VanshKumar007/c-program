#include <stdio.h>
#include <math.h>

int main() {
    int x1, y1, x2, y2;
    double distance;
    
    // Read input coordinates
    scanf("%d %d %d %d", &x1, &y1, &x2, &y2);
    
    // Calculate Euclidean distance
    distance = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));
    
    // Print result with 2 decimal places
    printf("%.2f\n", distance);
    
    return 0;
}
