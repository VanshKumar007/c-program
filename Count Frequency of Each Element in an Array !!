#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, a[100], counted[100] = {0};
    scanf("%d", &n);

    for(int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    for(int i = 0; i < n; i++) {
        if(counted[i]) 
            continue;  

        int count = 1;
        for(int j = i + 1; j < n; j++) {
            if(a[i] == a[j]) {
                count++;
                counted[j] = 1; // mark as counted
            }
        }
        printf("%d: %d\n", a[i], count);
    }

    return 0;
}
