#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c);      // rows, columns input

    int a[100][100];             // 2D array

    // input lena
    for(int i = 0; i < r; i++) {
        for(int j = 0; j < c; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    // row-wise sum
    for(int i = 0; i < r; i++) {
        int rowSum = 0;
        for(int j = 0; j < c; j++) {
            rowSum += a[i][j];
        }
        printf("%d\n", rowSum);
    }

    // column-wise sum
    for(int j = 0; j < c; j++) {
        int colSum = 0;
        for(int i = 0; i < r; i++) {
            colSum += a[i][j];
        }
        printf("%d\n", colSum);
    }

    return 0;
}
